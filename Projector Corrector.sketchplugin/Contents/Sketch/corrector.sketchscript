@import 'library/utilities.js'

function toggleOverlay(context) {
	com.gino.init(context);
	com.gino.toggleOverlay();
}

function increaseOverlay(context) {
	com.gino.init(context);
	com.gino.offsetOverlayAlpha('+');
}

function decreaseOverlay(context) {
	com.gino.init(context);
	com.gino.offsetOverlayAlpha('-');
}